{
    "name": "ld-ui-react",
    "version": "1.0.0",
    "description": "React UIs to visualize Linked Data, especially Ontology Design Patterns",
    "main": "lib/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "lint": "eslint --ignore-path .gitignore -c .eslintrc \"./**/*.js\"",
        "encode-icon": "cd ./src/icon/images && ./encode-all-svg && cd ../../.."
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Christian-Nja/ld-ui-react.git"
    },
    "keywords": [
        "linked",
        "data",
        "react",
        "ui",
        "ontology",
        "design",
        "patterns"
    ],
    "author": "Christian Colonna",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Christian-Nja/ld-ui-react/issues"
    },
    "homepage": "https://github.com/Christian-Nja/ld-ui-react#readme",
    "dependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@christian-nja/general-components": "^1.0.2",
        "@elfalem/leaflet-curve": "^0.6.0",
        "@fortawesome/fontawesome-free": "^5.14.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-brands-svg-icons": "^5.14.0",
        "@fortawesome/free-regular-svg-icons": "^5.14.0",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "leaflet": "^1.6.0",
        "leaflet-arc": "^1.0.2",
        "leaflet-arrowheads": "^1.2.1",
        "leaflet.markercluster": "^1.4.1",
        "overlapping-marker-spiderfier-leaflet": "^0.2.7",
        "prunecluster": "^2.1.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^7.7.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.13",
        "mini-svg-data-uri": "^1.2.3",
        "minimist": "^1.2.5",
        "svgo": "^1.3.2"
    }
}
