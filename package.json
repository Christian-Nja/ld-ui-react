{
    "name": "ld-ui-react",
    "version": "1.0.1",
    "description": "React UIs to visualize Linked Data, especially Ontology Design Patterns",
    "main": "lib/index.js",
    "module": "es/index.js",
    "jsnext:main": "es/index.js",
    "scripts": {
        "dev": "watch 'npm run build' ./src",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build:commonjs": "rm -rf ./lib && cross-env BABEL_ENV=commonjs ./node_modules/.bin/babel src --out-dir lib --copy-files",
        "build:es": "rm -rf ./es && cross-env BABEL_ENV=es babel src --out-dir es",
        "build": "npm run build:commonjs && npm run build:es",
        "lint": "eslint --ignore-path .gitignore -c .eslintrc \"./**/*.js\"",
        "encode-icon": "cd ./src/icon/images && ./encode-all-svg && cd ../../..",
        "docs": "jsdoc -c .jsdoc.conf.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Christian-Nja/ld-ui-react.git"
    },
    "keywords": [
        "linked",
        "data",
        "react",
        "ui",
        "ontology",
        "design",
        "patterns"
    ],
    "author": "Christian Colonna",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Christian-Nja/ld-ui-react/issues"
    },
    "homepage": "https://github.com/Christian-Nja/ld-ui-react#readme",
    "dependencies": {
        "@ant-design/icons": "^4.3.0",
        "@antv/graphin": "^1.5.0",
        "@antv/graphin-components": "^1.5.0",
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@christian-nja/general-components": "^1.0.2",
        "@elfalem/leaflet-curve": "^0.6.0",
        "@fortawesome/fontawesome-free": "^5.15.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-brands-svg-icons": "^5.15.1",
        "@fortawesome/free-regular-svg-icons": "^5.15.1",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.13",
        "@ldflex/comunica": "^3.4.2",
        "@rdfjs/data-model": "^1.2.0",
        "chroma-js": "^2.1.0",
        "d3": "^6.2.0",
        "ldflex": "^2.11.1",
        "leaflet": "^1.7.1",
        "leaflet-arc": "^1.0.2",
        "leaflet-arrowheads": "^1.2.2",
        "leaflet.markercluster": "^1.4.1",
        "overlapping-marker-spiderfier-leaflet": "^0.2.7",
        "pondjs": "^0.8.9",
        "prop-types": "^15.7.2",
        "prunecluster": "^2.1.0",
        "react": "^16.13.1",
        "react-compound-slider": "^3.3.1",
        "react-dom": "^16.13.1",
        "react-minimal-pie-chart": "^8.1.0",
        "semantic-ui-react": "^2.0.1",
        "underscore": "^1.12.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "babel-eslint": "^10.1.0",
        "better-docs": "^2.3.2",
        "cssnano": "^4.1.10",
        "eslint": "^7.14.0",
        "eslint-plugin-react": "^7.21.5",
        "husky": "^4.3.0",
        "jsdoc": "^3.6.6",
        "lint-staged": "^10.5.2",
        "mini-svg-data-uri": "^1.2.3",
        "minimist": "^1.2.5",
        "svgo": "^1.3.2",
        "watch": "^1.0.2"
    }
}
