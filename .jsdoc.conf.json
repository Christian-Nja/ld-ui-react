{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["./node_modules/better-docs/component"],
    "templates": {
        "better-docs": {
            "name": "LD-UI-REACT"
        }
    },
    "opts": {
        "destination": "docs",
        "encoding": "utf8",
        "recurse": true,
        "readme": "README.md",
        "verbose": true,
        "template": "./node_modules/better-docs"
    }
}
